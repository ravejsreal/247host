{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      },
      {
        "src": "index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/upload",
        "dest": "/app.py"
      },
      {
        "src": "/stop",
        "dest": "/app.py"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ]
  }
  